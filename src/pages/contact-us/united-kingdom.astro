---
import Base from "@/layouts/Base.astro";
import { markdownify } from "@/lib/utils/textConverter";
import { getEntry } from "astro:content";

const unitedkingdon = await getEntry("united-kingdom", "-index");
const { Content } = await unitedkingdon.render();
const { title, description, meta_title } = unitedkingdon.data;
---

<Base title={title} meta_title={meta_title} description={description}>
<!-- Heading started  -->

  <div class="relative" x-data="{ state: false }">
    <div
      class="absolute inset-0 blur-xl h-[580px]"
      style=" background: linear-gradient(
          143.6deg,
          rgba(192, 132, 252, 0) 20.79%,
          rgb(232, 245, 233) 10.92%,
          rgba(204, 171, 238, 0) 70.35%
        );
      "
    ></div>
    <div class="relative">
     
      <!-- Section -->
      <section>
        <div
          class="max-w-screen-xl mx-auto px-4 py-28 gap-12 text-gray-600 overflow-hidden md:px-8 md:flex"
        >
          <div class="flex-none space-y-5 max-w-xl pt-16">
            
            <h1 class="text-4xl text-gray-800 font-extrabold sm:text-5xl">
Contact Us            </h1>
            <p>
              Get in touch with Edhi Foundation for inquiries, assistance, and donations. Our dedicated team is here to support you.


            </p>
            <div class="flex items-center gap-x-3 sm:text-sm pt-8">
              
              
            </div>
          </div>
          <div class="flex-1 hidden md:block">
            <!-- Replace with your image -->
            <img
              src="/images/contact-us.png"
              class="max-w-xl mt-[-90px]"

            />
          </div>
        </div>
      </section>
    </div>
  </div>
